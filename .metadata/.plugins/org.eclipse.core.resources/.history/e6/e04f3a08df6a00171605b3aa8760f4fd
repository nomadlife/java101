package first.pack;

public class ControlFlow {
	public static void main(String[] args) {
	
		int a = 10;
		if(a<11){
			System.out.println("A is less than 10");
		}
		else{
			System.out.println("A is not less than 10");
		}
		
		int b=15;
		if (b==13)
			System.out.println("b==13");
		else if ((b<=17)&&(b>=12))
			System.out.println("b ~~~ ");
		else 
			System.out.println("b~~~");
		
		
	}



